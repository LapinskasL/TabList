    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": false
      },
    "background": {
        "scripts": ["background.js"]
    },
    "icons": { 
        // "16": "icon16.png",
        // "48": "icon48.png",
       "128": "images/extensionIcons/extensionIcon128.png" 
