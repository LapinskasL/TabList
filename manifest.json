{
    "manifest_version": 2,
    "name": "TabList",
    "version": "1.0",
    "description": "Create lists of tabs and launch them in a new window.",
    "permissions": [ 
        //"webNavigation", 
        //"*://*/*",
        "storage" // for chrome.storage
    ],
    // "content_scripts": [
    //     {
    //         "matches": ["https://www.youtube.com/*", 
    //                     "http://www.youtube.com/*" ],
    //         "js": ["content.js"],
    //         "run_at": "document_end"
    //     }
    // ],
    "browser_action": {
        "default_icon": "images/extensionIcons/extensionIcon128.png",
        "default_popup": "popup/popup.html",
